@await Html.PartialAsync("_Toolbar", new BreadcrumbViewModel { Title = "خط تولید" })

<div id="kt_content_container" class="d-flex flex-column-fluid align-items-start container-xxl">
    <div class="content flex-row-fluid" id="kt_content">
        <!--begin::Row-->
        <div class="row g-5 g-xl-8">
            <!--begin::Col-->
            <div class="col-md-12">
                <!--begin::Row-->
                <div class="row gx-5 gx-xl-8 mb-5 mb-xl-8">
                    <!--begin::Tiles Widget 1-->
                    <div class="card bgi-no-repeat bgi-size-cover bgi-position-y-center card-xl-stretch shadow-lg">
                        <!--begin::Body-->
                        <div class="card-body p-6">
                            <div id="myDiagramDiv"
                                 style="width: 100%; height: 550px; display: inline-block; vertical-align: top; position: relative; -webkit-tap-highlight-color: rgba(255, 255, 255, 0); cursor: auto;">
                                <canvas tabindex="0" width="872" height="548"
                                        style="position: absolute; top: 0px; left: 0px; z-index: 2; user-select: none; touch-action: none; width: 872px; height: 548px; cursor: auto;"></canvas>
                                <div style="position: absolute; overflow: auto; width: 872px; height: 548px; z-index: 1;">
                                    <div style="position: absolute; width: 1px; height: 1px;"></div>
                                </div>
                            </div>
                        </div>
                        <!--end::Body-->
                    </div>
                    <!--end::Tiles Widget 1-->
                </div>
                <!--end::Row-->
            </div>
            <!--end::Col-->
        </div>
        <!--end::Row-->
    </div>
</div>

@section Scripts {
    <script type="text/javascript" src="/Scripts/Diagram/Diagram.js"></script>
}


